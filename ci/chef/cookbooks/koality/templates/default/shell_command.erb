#!/bin/bash --login
source /home/<%= @user %>/scripts/setup.sh
cd /home/<%= @user %>/source/<%= @path %>
timeout <%= @timeout %> bash -c "
<% for @command in @validated_commands %>
source /home/<%= @user %>/scripts/validator.sh <%= @command %> &&
<% end %>
true"
r=$?
if [ $r -eq 124 ]
	then echo "<%= @name %> timed out after <%= @timeout %> seconds"
fi
exit $r
